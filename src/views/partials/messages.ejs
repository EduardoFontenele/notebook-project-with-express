<% if(errors.length > 0) { %>
    <div class="error-messages">
        <% errors.forEach(erro => { %>
            <%= erro %> <br>
            <% }) %>
    </div>
<% } %>

<% if(success.length > 0) { %>
    <div class="success-messages">
        <% success.forEach(sucess => { %>
            <%= success %> <br>
            <% }) %>
    </div>
<% } %>